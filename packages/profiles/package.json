{
	"name": "@payvo/sdk-profiles",
	"version": "4.0.0-next.16",
	"description": "Profiles for @payvo/sdk",
	"license": "MIT",
	"contributors": [],
	"type": "module",
	"exports": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": [
		"/distribution"
	],
	"scripts": {
		"build": "pnpm run clean && tsc",
		"build:analyze": "webpack --config ../../webpack.analyze.cjs",
		"build:release": "webpack --config ../../webpack.config.cjs",
		"build:watch": "pnpm run clean && tsc -w",
		"clean": "rimraf .coverage distribution tmp",
		"test": "uvu -r tsm source .test.ts",
		"test:coverage": "c8 pnpm run test",
		"test:integration": "uvu -r tsm test/integration .test.js",
		"test:watch": "watchlist source -- pnpm run test",
		"testi": "uvu -r tsm source"
	},
	"dependencies": {
		"@payvo/sdk": "workspace:*",
		"@payvo/sdk-cryptography": "workspace:*",
		"@payvo/sdk-helpers": "workspace:*",
		"@payvo/sdk-intl": "workspace:*",
		"@payvo/sdk-markets": "workspace:*",
		"@vechain/picasso": "^2.1.1",
		"bip38": "^3.1.1",
		"inversify": "^6.0.1",
		"joi": "^17.4.2",
		"localforage": "^1.10.0",
		"node-cache": "^5.1.2",
		"p-queue": "^7.1.0",
		"p-retry": "^5.0.0",
		"reflect-metadata": "^0.1.13",
		"semver": "^7.3.5",
		"type-fest": "^2.6.0"
	},
	"devDependencies": {
		"@payvo/sdk-ada": "workspace:*",
		"@payvo/sdk-ark": "workspace:*",
		"@payvo/sdk-btc": "workspace:*",
		"@payvo/sdk-eth": "workspace:*",
		"@payvo/sdk-http-fetch": "workspace:*",
		"@payvo/sdk-lsk": "workspace:*",
		"@payvo/sdk-test": "workspace:*",
		"@types/bip38": "2.0.1",
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^16.11.10",
		"@types/semver": "7.3.9",
		"fs-extra": "^10.0.0"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"engineStrict": true
}
